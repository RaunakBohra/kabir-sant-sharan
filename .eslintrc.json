{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "@next/next/no-img-element": "off",
    "react/no-unescaped-entities": "error",
    "react/jsx-key": "error",
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "ignorePatterns": [
    ".next/**",
    "node_modules/**",
    "dist/**",
    "build/**"
  ]
}